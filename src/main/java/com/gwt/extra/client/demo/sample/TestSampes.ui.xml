<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:usf.gwt.bootstrap.ui.widget" 
	xmlns:d="urn:import:usf.gwt.bootstrap.extra.datetimepicker">
	
	<b:Layout>
		<b:GridRow>
			
			<b:GridCol size="MD_6" ui:field="main">
				<d:DateTimePicker ui:field="pick" b:id="usf"/>
			</b:GridCol>
			
			<b:GridCol size="MD_6">
				<g:TextArea ui:field="log" readOnly="true"  width="100%" height="600px" text="log :" />
			</b:GridCol>
			
		</b:GridRow>
	</b:Layout>
	
</ui:UiBinder> 