<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:b="urn:import:usf.gwt.bootstrap.ui.widget" 
	xmlns:d="urn:import:usf.gwt.bootstrap.extra.datetimepicker"
	xmlns:t="urn:import:com.gwt.extra.client.demo.tools">

	<b:Layout>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker locale="en"/>
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker locale="en"/>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
	
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker format="MM_YYYY"/>
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker format="MM_YYYY"/>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker format="HH_MM" stepping="30"/>
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker format="HH_MM" stepping="30"/>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker showClearButton="true" showCloseButton="true" showTodayButton="true" />
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker showClearButton="true" 
						showCloseButton="true" showTodayButton="true" />
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker showWeeksNumber="true" />
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker showWeeksNumber="true" />
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>

		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker sideBySide="true" format="DD_MM_YYYY_HH_MM_SS"/>
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker sideBySide="true" format="DD_MM_YYYY_HH_MM_SS"/>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<d:DateTimePicker date="12/2016" format="MM_YYYY"/>
			</b:GridCol>
		
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<DateTimePicker date="12/2016" format="MM_YYYY"/>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		
		
	</b:Layout>



</ui:UiBinder>
