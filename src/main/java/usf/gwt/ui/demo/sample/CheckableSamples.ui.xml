<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:usf.gwt.ui.bootstrap.client"
	xmlns:t="urn:import:usf.gwt.ui.demo.tools">
	
	<b:Layout>
		<b:GridRow>
			<b:GridCol size="MD_4">
				<b:CheckBox text="Check_1" />
				<b:CheckBox text="Check_2" checked="true" />
				<b:CheckBox text="Check_3" enabled="false"/>
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<CheckBox text="Check_1" />
					<CheckBox text="Check_2" checked="true" />
					<CheckBox text="Check_3" enabled="false" />
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		<b:GridRow>
			<b:GridCol size="MD_4">
				<b:Radio text="radio_1" name="radio" />
				<b:Radio text="radio_2" name="radio" checked="true" />
				<b:Radio text="radio_3" name="radio" enabled="false"/>
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<Radio text="radio_1" name="radio" />
					<Radio text="radio_2" name="radio" checked="true" />
					<Radio text="radio_3" name="radio" enabled="false" />
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>	
	</b:Layout>	

</ui:UiBinder> 