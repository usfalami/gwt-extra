<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:b="urn:import:usf.gwt.ui.bootstrap.client" 
	xmlns:t="urn:import:usf.gwt.ui.demo.tools" 
	xmlns:d="urn:import:usf.gwt.ui.bootstrap.datetimepicker2">

	<b:Layout>
		
		<b:GridRow>
			<b:GridCol size="MD_10">
				<d:MonthCalendar diplayedMonth="0, 2015" />
				<d:MonthCalendar diplayedMonth="11, 2000" />
				<d:MonthCalendar diplayedMonth="9, 1987" />
				<d:MonthCalendar diplayedMonth="1, 2025" />
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<MonthCalendar diplayedMonth="0, 2015" />
					<MonthCalendar diplayedMonth="11, 2000" />
					<MonthCalendar diplayedMonth="9, 1987" />
					<MonthCalendar diplayedMonth="1, 2025" />
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_12">
				<d:YearCalendar displayedYear="2015" ui:field="c_2015"/>
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<YearCalendar displayedYear="2015"/>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>

	</b:Layout>

</ui:UiBinder>
