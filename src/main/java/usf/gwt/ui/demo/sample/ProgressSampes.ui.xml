<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:usf.gwt.ui.bootstrap.client" 
	xmlns:t="urn:import:usf.gwt.ui.demo.tools">

	<b:Layout>

		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<b:Progress>
					<b:ProgressBar value="25%" />
				</b:Progress>			
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<Progress>
						<ProgressBar value="25%" />
					</Progress>
				</t:XmlCode>			
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<b:Progress>
					<b:ProgressBar style="DANGER" text="85%" value="85%" />
				</b:Progress>
				<b:Progress>
					<b:ProgressBar style="SUCCESS" text="15%" value="15%" />
				</b:Progress>
				<b:Progress>
					<b:ProgressBar style="WARNING" text="59%" value="59%" />
				</b:Progress>
				<b:Progress>
					<b:ProgressBar style="INFO" text="80%" value="80%" />
				</b:Progress>
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<Progress>
						<ProgressBar style="DANGER" text="85%" value="85%" />
					</Progress>
					<Progress>
						<ProgressBar style="SUCCESS" text="15%" value="15%" />
					</Progress>
					<Progress>
						<ProgressBar style="WARNING" text="59%" value="59%" />
					</Progress>
					<Progress>
						<ProgressBar style="INFO" text="80%" value="80%" />
					</Progress>
				</t:XmlCode>			
			</b:GridCol>
		</b:GridRow>
		
		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<b:Progress>
					<b:ProgressBar text="Downloading 70%" value="70%" active="true" />
				</b:Progress>			
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<Progress>
						<ProgressBar text="Downloading 70%" value="70%" active="true" />
					</Progress>
				</t:XmlCode>			
			</b:GridCol>
		</b:GridRow>

		<b:GridRow>
			<b:GridCol size="MD_4" addStyleNames="exempleAlign">
				<b:Progress>
					<b:ProgressBar style="DANGER" text="Step 1" value="25%" />
					<b:ProgressBar style="SUCCESS" text="Step 2" value="15%" />					
					<b:ProgressBar style="WARNING" text="Step 3" value="49%" />
				</b:Progress>
			</b:GridCol>
			<b:GridCol size="MD_8">
				<t:XmlCode>
					<Progress>
						<ProgressBar style="DANGER" text="Step 1" value="25%" />
						<ProgressBar style="SUCCESS" text="Step 2" value="15%" />
						<ProgressBar style="WARNING" text="Step 3" value="49%" />
					</Progress>
				</t:XmlCode>
			</b:GridCol>
		</b:GridRow>

	</b:Layout>
	
</ui:UiBinder> 